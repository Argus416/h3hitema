const calendar = [];
	const today = moment();
	const startDay = today.clone().startOf("month").startOf("week");
	const endDay = today.clone().endOf("month").endOf("week");

	let datee = startDay.clone().subtract(1, "day");

	while (datee.isBefore(endDay, "day"))
		calendar.push(
			Array(7)
				.fill(0)
				.map(() => datee.add(1, "day").clone())
		);

	console.log(calendar);